{
  "mappings": {
    "real-server.internal.corp": "fake-server.example.test",
    "192.168.1.100": "11.22.33.44",
    "my-api-key-abc123": "FAKE_API_KEY",
    "password123": "FAKE_PASSWORD"
  },
  "excludePaths": [
    ".git",
    "node_modules",
    ".claude",
    "bin",
    "obj",
    "__pycache__",
    "venv",
    ".venv"
  ],
  "excludeExtensions": [
    ".exe",
    ".dll",
    ".pdb",
    ".png",
    ".jpg",
    ".jpeg",
    ".gif",
    ".ico",
    ".zip",
    ".tar",
    ".gz"
  ],
  "patterns": {
    "ipv4": true,
    "hostnames": [
      "\\.internal\\.corp$",
      "\\.local$",
      "\\.private$"
    ]
  }
}
